{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "cross-fetch": "^3.1.4",
    "env-cmd": "^10.1.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local": "env-cmd -e local -r env.json yarn start",
    "start:dev": "env-cmd -e dev -r env.json yarn start",
    "start:staging": "env-cmd -e staging -r env.json yarn start",
    "start:prod": "env-cmd -e prod -r env.json yarn start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -e dev -r env.json yarn build",
    "build:staging": "env-cmd -e staging -r env.json yarn build",
    "build:prod": "env-cmd -e prod -r env.json yarn build",
    "test": "react-scripts test",
    "test:coverage": "yarn test --coverage",
    "eject": "react-scripts eject",
    "cypress": "cypress open --env environment=local",
    "cypress-headless": "cypress run --env environment=local",
    "cypress-headless:mock": "cypress run --env environment=local --spec cypress/**/*.mock.spec.js",
    "cypress-headless:e2e": "cypress run --env environment=local --spec cypress/**/*.e2e.spec.js",
    "lint": "eslint --fix src/**/*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^7.7.0",
    "eslint": "7.30.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-react": "7.24.0"
  }
}
